{
  "name": "fd-fetch-wrapper",
  "version": "0.1.0",
  "description": "Simple wrapper around fetch function to manage in a simple way the errors from BE using http status code",
  "author": "alessandro gambaro",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/AlessandroGambaro/fdFetchWrapper",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/cli": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "react-scripts": "3.4.3"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "peerDependencies": {},
  "scripts": {
    "distribute": "rmdir /s /q dist & mkdir dist && npx babel src/lib -d dist --copy-files",
    "start": "react-scripts start"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
